set, DataLoader\n","from numpy import genfromtxt\n","\n","class ImdbDataset(Dataset):\n","  def __init__(self, csv_path_label,csv_path_target):\n","    super(ImdbDataset, self).__init__()\n","    self.x = genfromtxt(csv_path_label, delimiter=',')\n","    self.target = genfromtxt(csv_path_target, delimiter=',')\n","\n","    self.target = torch.from_numpy(self.target).type(torch.LongTensor)\n","    self.x      = torch.from_numpy(self.x).type(torch.FloatTensor)\n","\n","  def __len__(self):\n","    return self.x.shape[0]\n","  \n","  def __getitem__(self, index):\n","    return self.x[index], self.target[index]"],"execution_count":0,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"FQB9aBTwhAf-","colab_type":"text"},"source":["# Definição do modelo\n","\n"]},{"cell_type":"markdown","metadata":{"id":"mK-PdWh7b8G4","colab_type":"text"},"source":["Modelo"]},{"cell_type":"code","metadata":{"id":"ZdfM7We6eqIK","colab_type":"code","colab":{}},"source":["class Model(torch.nn.Module):\n","    def __init__(self):\n","        super(Model, self).__init__()\n","        self.layer1 = torch.nn.Linear(in_features=18668, out_features=50)   \n","        self.layer2 = torch.nn.Linear(in_features=50, out_features=2)   \n","\n","    def forward(self, x):\n","        x = self.layer1(x)\n","        x = F.relu(x)\n","        x = self.layer2(x)\n","        return x"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"oUqVVOEo14Ya","colab_type":"code","outputId":"cc45a525-50bb-4d0f-f81b-c98aa875da54","executionInfo":{"status":"ok","timestamp":1584555487162,"user_tz":180,"elapsed":15891,"user":{"displayName":"Matheus Gustavo Alves Sasso","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Gh3hBAk_xyvoNPG1oRcGua6jw923vKztFHNgbSr=s64","userId":"12802130788492198743"}},"colab":{"base_uri":"https://localhost:8080/","height":84}},"source":["#Visualuzando modelo\n","model = Model()\n","model"],"execution_count":12,"outputs":[{"output_type":"execute_result","data":{"text/p